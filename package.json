    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.24",
    "postcss-preset-mantine": "^1.3.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.18",
    "storybook-dark-mode": "^3.0.0",
    "ts-jest": "^29.1.0",
